<nodeDefs>
    <nodeDef id="controller" nls="ctl">
        <editors />
        <sts>
            <st id="ST" editor="bool" />
        </sts>
        <cmds>
            <sends>
                <cmd id="DON" />
                <cmd id="DOF" />
             </sends>   
            <accepts>
                <cmd id="DISCOVERY" />
            </accepts>
        </cmds>
    </nodeDef>
    <nodeDef id="WLED" nls="MGR">
        <editors />
        <sts>
        	<st id="CUSTOMAPI" editor="MCUSTOMAPI" /> <!-- Custom API Command -->
            <st id="ST" editor="MONOFF" />
            <st id="GV3" editor="MCLBRI" /> <!-- Brightness -->
            <st id="GV6" editor="MCOLOR" />
            <st id="GV7" editor="MCOLOR" />
            <st id="GV8" editor="MCOLOR" />
            <st id="GV4" editor="MEFFECT" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="QUERY" />
                <cmd id="DON" />
                <cmd id="DOF" />
                <cmd id="SET_PROFILE" />
                <cmd id="SET_COLORID">
                    <p id="R" editor="MCOLOR" init="GV6" />
                    <p id="G" editor="MCOLOR" init="GV7" />
                    <p id="B" editor="MCOLOR" init="GV8" />
                </cmd>
                <cmd id="SET_BRI">
                    <p id="" editor="MCLBRI" init="GV3" />
                </cmd>
                <cmd id="SET_EFFECT">
                    <p id="" editor="MEFFECT" />
                </cmd>
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>